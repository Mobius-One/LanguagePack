        <Keyboard nameResId="@string/keitai_katakana"
        iconResId="change_me"
        layoutResId="@xml/jp_keitaikatakana"
        id="2ec3710-c666-11e8-b568-020"
        defaultDictionaryLocale="jp"
        description="12-key Katakana keyboard based on classic multitap keitai."
        index="5"/>
    </Keyboards>